{
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "func-names": ["error", "never"],
    "class-methods-use-this": [
      "error",
      {
        "exceptMethods": ["visitFieldDefinition"]
      }
    ]
  }
}
